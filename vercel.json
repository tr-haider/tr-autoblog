{
  "version": 2,
  "name": "autoblog-ai",
  "buildCommand": "npm run vercel:build",
  "outputDirectory": "blog-ui/build",
  "installCommand": "npm run install:all",
  "functions": {
    "autoblog-api/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/autoblog-api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
